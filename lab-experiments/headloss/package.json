{
  "name": "headloss",
  "version": "1.0.0",
  "description": "A virtual laboratory experiment based on the head loss experiment of the chemical engineering department at Washington State University",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx webpack --config ./_dev/webpack.config.js --mode=production --progress",
    "watch": "npx webpack --watch --config ./_dev/webpack.config.js --mode=development --progress",
    "start": "npx webpack serve --config ./_dev/webpack.config.js --mode=development --open"
  },
  "keywords": [
    "simulation",
    "chemical-engineering",
    "virtual-laboratory"
  ],
  "author": "Neil Hendren",
  "license": "MIT",
  "devDependencies": {
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.0",
    "raw-loader": "^4.0.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
